<div class="section row">
  <div class="col s12 m6">
    <div class="card">
      <div class="card-content">
        <p>
          <strong>Name:</strong>
          <%= @user.name %>
        </p>

        <p>
          <strong>Email:</strong>
          <%= mail_to @user.email %>
        </p>
      </div>

      <div class="card-action">
        <%= link_to 'Edit', edit_admin_user_path(@user), class: :btn %>
      </div>
    </div>
  </div>
</div>
