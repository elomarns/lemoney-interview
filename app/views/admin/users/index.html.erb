<h1 class="red-text text-lighten-2">Users</h1>

<table class="striped bordered responsive-table">
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>

  <tbody>
    <%= render @users %>
